{
  "Routes": [
    {
      "ServiceName": "SEN300BooksServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/booksserviceapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "SEN300BasketServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/basketserviceapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "SEN300OrdersServiceAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/ordersapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://SEN300APIGatewayOcelot:80",
    "RequestIdKey": "OcRequestId",
    "DownstreamScheme": "http",
    "ReRoutesCaseSensitive": false,
    "UseServiceDiscovery": true,
    "ServiceDiscoveryProvider": {
      "Host": "SEN300EurekaRegistry",
      "Port": 8761,
      "Type": "Eureka"
    }
  }
}