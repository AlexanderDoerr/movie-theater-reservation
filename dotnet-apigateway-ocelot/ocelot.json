{
  "Routes": [
    {
      "ServiceName": "OrderServiceClientAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/ordersapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "UserServiceClientAPI",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/usersapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    },
    {
      "ServiceName": "movie-service",
      "DownstreamPathTemplate": "/{everything}",
      "UpstreamPathTemplate": "/moviesapi/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "FileCacheOptions": {
        "TtlSeconds": 15
      },
      "LoadBalancerOptions": {
        "Type": "LeastConnection",
        "Key": null,
        "Expiry": 0
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5041",
    "RequestIdKey": "OcRequestId",
    "DownstreamScheme": "http",
    "ReRoutesCaseSensitive": false,
    "UseServiceDiscovery": true,
    "ServiceDiscoveryProvider": {
      "Host": "consul",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}